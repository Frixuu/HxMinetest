{
    "defaultSeverity": "INFO",
    "extendsConfigPath": "",
    "baseDefines": [],
    "defineCombinations": [],
    "checks": [
        {
            "type": "Anonymous",
            "props": {
                "severity": "IGNORE"
            }
        },
        {
            "type": "ArrowFunction",
            "props": {
                "allowReturn": true,
                "allowFunction": true,
                "allowCurlyBody": true,
                "allowSingleArgParens": true
            }
        },
        {
            "type": "AvoidStarImport",
            "props": {
                "severity": "WARNING"
            }
        },
        {
            "type": "AvoidTernaryOperator",
            "props": {
                "severity": "IGNORE"
            }
        },
        {
            "type": "BlockBreakingConditional",
            "props": {}
        },
        {
            "type": "CodeSimilarity",
            "props": {
                "severityIdentical": "WARNING",
                "thresholdIdentical": 75,
                "thresholdSimilar": 110
            }
        },
        {
            "type": "CommentedOutCode",
            "props": {
                "severity": "WARNING"
            }
        },
        {
            "type": "ConditionalCompilation",
            "props": {
                "policy": "aligned"
            }
        },
        {
            "type": "CyclomaticComplexity",
            "props": {
                "thresholds": [
                    {
                        "severity": "WARNING",
                        "complexity": 11
                    },
                    {
                        "severity": "ERROR",
                        "complexity": 21
                    }
                ]
            }
        },
        {
            "type": "DefaultComesLast",
            "props": {
                "severity": "ERROR"
            }
        },
        {
            "type": "DocCommentStyle",
            "props": {
                "startStyle": "twostars",
                "lineStyle": "none"
            }
        },
        {
            "type": "Dynamic",
            "props": {
                "severity": "IGNORE"
            }
        },
        {
            "type": "EmptyBlock",
            "props": {
                "option": "empty",
                "severity": "WARNING"
            }
        },
        {
            "type": "EmptyLines",
            "props": {
                "max": 1,
                "allowEmptyLineAfterSingleLineComment": false,
                "allowEmptyLineAfterMultiLineComment": true,
                "requireEmptyLineAfterPackage": true,
                "requireEmptyLineAfterClass": false,
                "requireEmptyLineAfterInterface": false,
                "requireEmptyLineAfterAbstract": false
            }
        },
        {
            "type": "ExtendedEmptyLines",
            "props": {
                "max": 1,
                "skipSingleLineTypes": true,
                "defaultPolicy": "upto",
                "ignore": [],
                "none": [
                    "beforePackage",
                    "typeDefinition",
                    "beforeRightCurly",
                    "typeDefinition"
                ],
                "exact": [
                    "beforeFileEnd"
                ],
                "upto": [
                    "betweenImports",
                    "beforeUsing",
                    "inFunction",
                    "afterLeftCurly"
                ],
                "atleast": []
            }
        },
        {
            "type": "FieldDocComment",
            "props": {
                "severity": "IGNORE",
                "fieldType": "BOTH",
                "modifier": "PUBLIC",
                "requireParams": false,
                "requireReturn": false,
                "ignoreOverride": true,
                "excludeNames": [
                    "new",
                    "toString"
                ],
                "tokens": [
                    "ABSTRACT_DEF",
                    "CLASS_DEF",
                    "ENUM_DEF",
                    "INTERFACE_DEF",
                    "TYPEDEF_DEF"
                ]
            }
        },
        {
            "type": "Final",
            "props": {}
        },
        {
            "type": "HiddenField",
            "props": {
                "ignoreConstructorParameter": true,
                "ignoreSetter": false,
                "ignoreFormat": "^(main|run)$"
            }
        },
        {
            "type": "Indentation",
            "props": {
                "character": "    ",
                "ignoreConditionals": false,
                "conditionalPolicy": "aligned",
                "ignoreComments": false,
                "wrapPolicy": "larger"
            }
        },
        {
            "type": "IndentationCharacter",
            "props": {
                "character": "space",
                "ignorePattern": "^$"
            }
        },
        {
            "type": "InnerAssignment",
            "props": {
                "severity": "ERROR",
                "ignoreReturnAssignments": false
            }
        },
        {
            "type": "Interface",
            "props": {
                "allowProperties": true,
                "allowMarkerInterfaces": true,
                "severity": "INFO"
            }
        },
        {
            "type": "LeftCurly",
            "props": {
                "tokens": [
                    "CLASS_DEF",
                    "ENUM_DEF",
                    "ABSTRACT_DEF",
                    "INTERFACE_DEF",
                    "FUNCTION",
                    "FOR",
                    "IF",
                    "WHILE",
                    "SWITCH",
                    "TRY",
                    "CATCH"
                ],
                "option": "eol",
                "ignoreEmptySingleline": true,
                "ignoreSingleline": false
            }
        },
        {
            "type": "LeftCurly",
            "props": {
                "tokens": [
                    "TYPEDEF_DEF"
                ],
                "option": "eol",
                "ignoreEmptySingleline": false,
                "ignoreSingleline": false
            }
        },
        {
            "type": "LineLength",
            "props": {
                "max": 100,
                "ignorePattern": "^$"
            }
        },
        {
            "type": "LocalVariableName",
            "props": {
                "severity": "ERROR",
                "format": "^[a-z][a-zA-Z0-9]*$"
            }
        },
        {
            "type": "MethodCount",
            "props": {
                "maxTotal": 15,
                "maxPrivate": 10,
                "maxPublic": 10
            }
        },
        {
            "type": "MethodLength",
            "props": {
                "max": 65,
                "ignoreEmptyLines": true
            }
        },
        {
            "type": "ModifierOrder",
            "props": {
                "severity": "WARNING",
                "modifiers": [
                    "MACRO",
                    "PUBLIC_PRIVATE",
                    "STATIC",
                    "INLINE",
                    "OVERRIDE",
                    "DYNAMIC"
                ]
            }
        },
        {
            "type": "NestedControlFlow",
            "props": {
                "max": 3
            }
        },
        {
            "type": "NestedForDepth",
            "props": {
                "max": 1
            }
        },
        {
            "type": "NestedIfDepth",
            "props": {
                "max": 1
            }
        },
        {
            "type": "NullableParameter",
            "props": {
                "option": "questionMark"
            }
        },
        {
            "type": "OperatorWhitespace",
            "props": {
                "assignOpPolicy": "around",
                "unaryOpPolicy": "none",
                "ternaryOpPolicy": "ignore",
                "arithmeticOpPolicy": "around",
                "compareOpPolicy": "around",
                "bitwiseOpPolicy": "ignore",
                "boolOpPolicy": "around",
                "intervalOpPolicy": "ignore",
                "arrowPolicy": "around",
                "arrowFunctionPolicy": "around",
                "oldFunctionTypePolicy": "ignore",
                "newFunctionTypePolicy": "around"
            }
        },
        {
            "type": "OperatorWrap",
            "props": {
                "tokens": [
                    "=",
                    "+",
                    "-",
                    "*",
                    "/",
                    "%",
                    ">",
                    "<",
                    ">=",
                    "<=",
                    "==",
                    "!=",
                    "&",
                    "|",
                    "^",
                    "&&",
                    "||",
                    "<<",
                    ">>",
                    ">>>",
                    "+=",
                    "-=",
                    "*=",
                    "/=",
                    "%=",
                    "<<=",
                    ">>=",
                    ">>>=",
                    "|=",
                    "&=",
                    "^=",
                    "...",
                    "=>",
                    "++",
                    "--"
                ],
                "option": "nl"
            }
        },
        {
            "type": "ParameterNumber",
            "props": {
                "max": 5,
                "ignoreOverriddenMethods": false
            }
        },
        {
            "type": "PublicAccessor",
            "props": {
                "severity": "INFO"
            }
        },
        {
            "type": "RedundantAccessMeta",
            "props": {
                "prohibitMeta": false
            }
        },
        {
            "type": "RedundantAllowMeta",
            "props": {
                "prohibitMeta": false
            }
        },
        {
            "type": "RedundantModifier",
            "props": {
                "enforcePublicPrivate": true
            }
        },
        {
            "type": "Return",
            "props": {
                "allowEmptyReturn": true,
                "enforceReturnType": false,
                "enforceReturnTypeForAnonymous": false
            }
        },
        {
            "type": "ReturnCount",
            "props": {
                "max": 5,
                "ignoreFormat": "^$",
                "severity": "WARNING"
            }
        },
        {
            "type": "RightCurly",
            "props": {
                "tokens": [
                    "CLASS_DEF",
                    "ENUM_DEF",
                    "ABSTRACT_DEF",
                    "TYPEDEF_DEF",
                    "INTERFACE_DEF",
                    "OBJECT_DECL",
                    "FUNCTION",
                    "FOR",
                    "WHILE",
                    "SWITCH"
                ],
                "option": "aloneorsingle"
            }
        },
        {
            "type": "RightCurly",
            "props": {
                "tokens": [
                    "CATCH"
                ],
                "option": "aloneorsingle"
            }
        },
        {
            "type": "RightCurly",
            "props": {
                "tokens": [
                    "IF"
                ],
                "option": "same"
            }
        },
        {
            "type": "RightCurly",
            "props": {
                "tokens": [
                    "OBJECT_DECL"
                ],
                "option": "aloneorsingle"
            }
        },
        {
            "type": "SeparatorWhitespace",
            "props": {
                "dotPolicy": "none",
                "commaPolicy": "after",
                "allowTrailingComma": false,
                "semicolonPolicy": "after"
            }
        },
        {
            "type": "SeparatorWrap",
            "props": {
                "tokens": [
                    ","
                ],
                "option": "eol"
            }
        },
        {
            "type": "SimplifyBooleanReturn",
            "props": {
                "severity": "ERROR"
            }
        },
        {
            "type": "Spacing",
            "props": {
                "spaceAroundBinop": false,
                "noSpaceAroundUnop": false,
                "spaceIfCondition": "should",
                "spaceForLoop": "should",
                "spaceWhileLoop": "should",
                "spaceSwitchCase": "should",
                "spaceCatch": "should",
                "ignoreRangeOperator": false
            }
        },
        {
            "type": "StringLiteral",
            "props": {
                "policy": "doubleAndInterpolation",
                "allowException": false
            }
        },
        {
            "type": "TabForAligning",
            "props": {
                "severity": "ERROR"
            }
        },
        {
            "type": "TODOComment",
            "props": {
                "format": "^\\s*(TODO|FIXME|HACK|XXX|BUG)",
                "severity": "IGNORE"
            }
        },
        {
            "type": "Trace",
            "props": {
                "severity": "INFO"
            }
        },
        {
            "type": "TrailingWhitespace",
            "props": {}
        },
        {
            "type": "Type",
            "props": {
                "severity": "WARNING",
                "ignoreEnumAbstractValues": true
            }
        },
        {
            "type": "TypeDocComment",
            "props": {
                "severity": "IGNORE",
                "tokens": [
                    "ABSTRACT_DEF",
                    "CLASS_DEF",
                    "ENUM_DEF",
                    "INTERFACE_DEF",
                    "TYPEDEF_DEF"
                ]
            }
        },
        {
            "type": "UnnecessaryConstructor",
            "props": {
                "severity": "ERROR"
            }
        },
        {
            "type": "UnusedImport",
            "props": {
                "severity": "WARNING",
                "ignoreModules": [],
                "moduleTypeMap": {}
            }
        },
        {
            "type": "UnusedLocalVar",
            "props": {
                "severity": "WARNING"
            }
        },
        {
            "type": "VarTypeHint",
            "props": {
                "severity": "IGNORE",
                "ignoreEnumAbstractValues": true,
                "typeHintPolicy": "infer_all"
            }
        }
    ],
    "exclude": {},
    "version": 1
}